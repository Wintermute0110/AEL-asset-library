<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <category>SONY</category>
  <name>PSP (Retroarch)</name>
  <year>2004</year>
  <platform>Sony PlayStation Portable</platform>
  <application>/home/kodi/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/ppsspp_libretro.so -f -v &quot;$rom$&quot;</args>
  <args_extra></args_extra>
  <romext>cue|iso</romext>
  <rompath>/home/kodi/AEL-ROMs/sony-psp/</rompath>
  <path_assets>/home/kodi/AEL-assets/sony-psp</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
