<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <category>SNK</category>
  <name>NeoGeo CD (MAME)</name>
  <platform>SNK Neo Geo CD</platform>
  <application>mame</application>
  <args>neocd -cdrom &quot;$rom$&quot;</args>
  <args_extra>neocdz -cdrom &quot;$rom$&quot;</args_extra>
  <args_extra>neocdj -cdrom &quot;$rom$&quot;</args_extra>
  <romext>cue|chd</romext>
  <rompath>/home/kodi/AEL-ROMs/snk-neocd/</rompath>
  <path_assets>/home/kodi/AEL-assets/snk-neocd/</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
