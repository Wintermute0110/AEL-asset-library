<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <category>SEGA</category>
  <name>Game Gear (Retroarch)</name>
  <platform>Sega Game Gear</platform>
  <application>/home/kodi/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/genesis_plus_gx_libretro.so -f -v &quot;$rom$&quot;</args>
  <romext>zip</romext>
  <rompath>/home/kodi/AEL-ROMs/sega-gg/</rompath>
  <path_assets>/home/kodi/AEL-assets/sega-gg</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
