  <args_extra>neocd -cdrom &quot;$rom$&quot;</args_extra>
  <romext>cue|chd</romext>
  <rompath>/home/kodi/AEL-ROMs/snk-neocd/</rompath>
  <path_assets>/home/kodi/AEL-assets/snk-neocd/</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
