<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <category>Nintendo</category>
  <name>SNES (Retroarch)</name>
  <platform>Nintendo SNES</platform>
  <application>/home/kodi/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/bsnes_balanced_libretro.so -f -v &quot;$rom$&quot;</args>
  <args_extra>-L /home/kodi/bin/libretro/mednafen_snes_libretro.so -f -v &quot;$rom$&quot;</args_extra>
  <romext>zip</romext>
  <rompath>/home/kodi/AEL-ROMs/nintendo-snes/</rompath>
  <path_assets>/home/kodi/AEL-assets/nintendo-snes</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
