<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <category>SEGA</category>
  <name>Mega CD (Retroarch)</name>
  <year>1991</year>
  <platform>Sega MegaCD</platform>
  <application>/home/kodi/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/genesis_plus_gx_libretro.so -f -v &quot;$rom$&quot;</args>
  <args_extra>-L /home/kodi/bin/libretro/picodrive_libretro.so -f -v &quot;$rom$&quot;</args_extra>
  <romext>cue|iso</romext>
  <rompath>/home/kodi/AEL-ROMs/sega-megacd/</rompath>
  <path_assets>/home/kodi/AEL-assets/sega-megacd</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
