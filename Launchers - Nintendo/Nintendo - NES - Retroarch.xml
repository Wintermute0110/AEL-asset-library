<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <category>Nintendo</category>
  <name>NES (Retroarch)</name>
  <year>1983</year>
  <platform>Nintendo NES</platform>
  <application>/home/kodi/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/fceumm_libretro.so -f -v &quot;$rom$&quot;</args>
  <args_extra>-L /home/kodi/bin/libretro/nestopia_libretro.so -f -v &quot;$rom$&quot;</args_extra>
  <romext>zip</romext>
  <rompath>/home/kodi/AEL-ROMs/nintendo-nes/</rompath>
  <path_assets>/home/kodi/AEL-assets/nintendo-nes</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
