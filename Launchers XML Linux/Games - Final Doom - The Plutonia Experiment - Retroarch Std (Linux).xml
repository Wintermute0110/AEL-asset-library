<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!-- Standalone launcher for Doom using Retroarch -->
<advanced_emulator_launcher_configuration>
<launcher>
    <name>Final Doom - The Plutonia Experiment (Retroarch)</name>
    <category>Games</category>
    <Launcher_NFO>../Launcher assets Games/Final Doom Plutonia.nfo</Launcher_NFO>
    <Asset_Prefix>../Launcher assets Games/Final Doom Plutonia</Asset_Prefix>
    <platform>Unknown</platform>
    <application>/home/kodi/bin/retroarch</application>
    <args>-L /home/kodi/bin/libretro/prboom_libretro.so -v -f /home/kodi/AEL-ROMs/games-plutonia/plutonia.wad</args>
</launcher>
</advanced_emulator_launcher_configuration>
