<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--
    Launch Neo Geo CD ROMs using MAME Neo Geo CD Software List.
  -->
<advanced_emulator_launcher_configuration>
<launcher>
  <name>Neo Geo CD (MAME)</name>
  <category>SNK</category>
  <Launcher_NFO>../Launcher assets SNK/Neo Geo CD.nfo</Launcher_NFO>
  <platform>SNK Neo Geo CD</platform>
  <application>/usr/games/mame</application>
  <args>neocdz -cdrom &quot;$rom$&quot;</args>
  <args_extra>neocdzj -cdrom &quot;$rom$&quot;</args_extra>
  <args_extra>neocd -cdrom &quot;$rom$&quot;</args_extra>
  <ROM_path>/home/kodi/AEL-ROMs/snk-neocd/</ROM_path>
  <ROM_ext>cue|chd</ROM_ext>
  <ROM_asset_path>/home/kodi/AEL-assets/snk-neocd/</ROM_asset_path>
  <Asset_Prefix>Neo Geo CD</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>
