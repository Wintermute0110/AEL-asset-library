<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <name>Nintendo DS (Retroarch)</name>
  <category>Nintendo</category>
  <Launcher_NFO>../Launcher assets Nintendo/Nintendo DS.nfo</Launcher_NFO>
  <platform>DS</platform>
  <application>/home/kodi/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/desmume_libretro.so -f -v &quot;$rom$&quot;</args>
  <ROM_path>/home/kodi/AEL-ROMs/nintendo-ds/</ROM_path>
  <ROM_ext>nds</ROM_ext>
  <ROM_asset_path>/home/kodi/AEL-assets/nintendo-ds</ROM_asset_path>
  <Asset_Prefix>../Launcher assets Nintendo/Nintendo DS</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>
