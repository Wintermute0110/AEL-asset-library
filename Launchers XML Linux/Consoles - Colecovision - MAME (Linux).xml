<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
    <name>Colecovision (MAME)</name>
    <category>Consoles</category>
    <Launcher_NFO>../Launcher assets Consoles/Colecovision.nfo</Launcher_NFO>
    <platform>Coleco Colecovision</platform>
    <application>/home/kodi/bin/mame64</application>
    <args>coleco -cart &quot;$rom$&quot;</args>
    <args_extra>colecop -cart &quot;$rom$&quot;</args_extra>
    <ROM_path>/home/kodi/AEL-ROMs/console-cvision/</ROM_path>
    <ROM_ext>zip</ROM_ext>
    <ROM_asset_path>/home/kodi/AEL-assets/console-cvision/</ROM_asset_path>
    <Asset_Prefix>../Launcher assets Consoles/Colecovision</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>

