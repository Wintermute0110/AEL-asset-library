<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--
    Launch AES ROMs using MAME Neo Geo AES Software List.
  -->
<advanced_emulator_launcher_configuration>
<launcher>
  <name>Neo Geo AES (MAME)</name>
  <category>SNK</category>
  <Launcher_NFO>../Launcher assets SNK/Neo Geo AES.nfo</Launcher_NFO>
  <platform>MAME</platform>
  <application>/usr/games/mame</application>
  <args>aes -cart &quot;$rom$&quot;</args>
  <ROM_path>/home/kodi/AEL-ROMs/mame-neogeo/</ROM_path>
  <ROM_ext>cue|chd</ROM_ext>
  <ROM_asset_path>/home/kodi/AEL-assets/mame-neogeo/</ROM_asset_path>
  <Asset_Prefix>../Launcher assets SNK/Neo Geo AES</Asset_Prefix>
</launcher>
</advanced_emulator_launcher_configuration>
